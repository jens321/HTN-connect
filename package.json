{
  "name": "connect",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.1",
    "firebase": "^5.5.0",
    "node-sass": "^4.9.3",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "standard": "^12.0.1"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src/scss --include-path ./node-modules src/scss/ -o src/scss/",
    "watch-css": "npm run build-css && node-sass --include-path ./src/scss --include-path ./node-modules src/scss/ -o src/scss/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  }
}
